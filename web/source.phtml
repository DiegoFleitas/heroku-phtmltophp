    function validarregistro_ajax() {
        var email_address_register = $j('#register_header input#email_address').val();
        var email_address_confirm = $j('#register_header input#email_address_confirm').val();
        var password_register = $j('#register_header input#password').val();
        var gender_register = $j('#register_header #gender option:selected').val();
        var confirmation_register = $j('#register_header input#confirmation').val();
        var emailRegex = new RegExp(/^([\w\.\-]+)@([\w\-]+)((\.(\w){2,3})+)$/i);
        var validemail = emailRegex.test(email_address_register);
        $j('#register_header input').each(function(){
            var valueinput = $j(this).val();
            if (valueinput == ''){
                $j(this).addClass('error_msg_input');
                $j(this).parent().next().next().fadeIn('slow');
            }
        });
        var avanza = true;

        if (gender_register == ''){
            $j('#register_header select#gender').addClass('error_msg_input');
            avanza = false;
        }
        if (!validemail || email_address_register =='' ){
            $j('#register_header p.error_msg#email_address').hide();
            $j('#register_header input#email_address').addClass('error_msg_input');
            $j('#register_header p.error_msg#email_address_incorrecto').fadeIn('slow');
            avanza = false;
        }
        if (email_address_register !== email_address_confirm){
            $j('#register_header input#email_address').addClass('error_msg_input');
            $j('#register_header input#email_address_confirm').addClass('error_msg_input');
            $j('#register_header p.error_msg#email_address_nocoincide').fadeIn('slow');
            avanza = false;
        }
        if (password_register.length < 6){
            $j('#register_header input#password').addClass('error_msg_input');
            $j('#register_header input#password').parent().next().next().fadeIn('slow');
            avanza = false;
        }
        if (password_register !== confirmation_register){
            $j('#register_header input#confirmation').addClass('error_msg_input');
            $j('#register_header input#confirmation').parent().next().next().next().fadeIn('slow');
            avanza = false;
        }
        if(avanza==true){
            var registernews = 'yes';
            if ($j('#is_subscribed').is(':checked')) {
                registernews = 'yes';
            } else {
                registernews = 'no';
            }

            var urlactual = '<?php echo  $this->getUrl(); ?>';
            var validarregistro_checkout_ajax = jQuery.ajax({
                url: '<?php echo $this->getUrl('youama_ajaxlogin/ajax/index' , array('_secure' => Mage::app()->getFrontController()->getRequest()->isSecure())); ?>',
                type: 'POST',
                data: {
                    ajax : 'register',
                    licence : 'ok',
                    email : $j('#register_header input#email_address').val(),
                    firstname : $j('#register_header input#firstname').val(),
                    lastname : $j('#register_header input#lastname').val(),
                    newsletter : registernews,
                    gender : $j('#register_header #gender option:selected').val(),
                    password : $j('#register_header input#password').val(),
                    passwordsecond : $j('#register_header input#confirmation').val()
                },
                dataType: "html"
            });
            $j('#register_header .cargando_login').show();
            $j('#register_header #send3').hide();
            validarregistro_checkout_ajax.done(function(msg) {
                if (msg == 'success'){
                    //_gaq.push(['_trackEvent', 'Registro Usuario Header', 'Click']);
                    try{
                        ga('send', 'event', 'Registro Usuario Header', 'Click'); // Universal Analytics
                    }catch (ee){

                    }
                    window.location = urlactual;
                }
                else if (msg == 'emailisexist,') {
                    alert('Este email ya esta registrado en TiendaMIA');
                    $j('#register_header .cargando_login').hide();
                    $j('#register_header #send3').show();
                } else {
                    var splitmsg = new Array();
                    splitmsg = msg.split(',');

                    for (error in splitmsg) {
                        $j('.error_msg.'+splitmsg[error]).show()
                    }
                    $j('#register_header .cargando_login').hide();
                    $j('#register_header #send3').show();
                }
            });
            validarregistro_checkout_ajax.fail(function(jqXHR, textStatus, errorThrown) {
                //alert('Error!')
                window.location = urlactual;
            });
        }
    }
