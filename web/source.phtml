    $j("#register_header input#email_address_confirm").on('input', function(){
        var email = $j('#register_header input#email_address').val();
        var emailRegex = new RegExp(/^([\w\.\-]+)@([\w\-]+)((\.(\w){2,3})+)$/i);
        var emailConfirm = $j(this).val();
        if (email == emailConfirm && emailRegex.test(email)) {
            $j(this).addClass('valid');
            $j('#email_address_nocoincide').hide();
            $j("#register_header input#email_address").removeClass('error_msg_input');
        } else {
            $j(this).removeClass('valid');
        }
    });
    $j('#register_header input#password').on('input', function(){
        var pass = $j(this).val();
        var confirmacion = $j('#register_header input#confirmation');
        if (pass.length > 5 && pass.length < 17) {
            $j(this).addClass('valid');
            if(pass == confirmacion.val()){
                confirmacion.addClass('valid');
                confirmacion.removeClass('error_msg_input');
                confirmacion.parent().siblings('p').hide();
            }
        } else {
            $j(this).removeClass('valid');
            confirmacion.removeClass('valid');
        }
    });
    $j('#register_header input#confirmation').on('input', function(){
        var pass = $j('#register_header input#password').val();
        var passConfirm = $j('#register_header input#confirmation').val();
        if (pass == passConfirm && pass.length > 5 && pass.length < 17) {
            $j(this).addClass('valid');
        } else {
            $j(this).removeClass('valid');
        }
    });

